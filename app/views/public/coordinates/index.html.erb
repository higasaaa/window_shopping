<h1>Public::Coordinates#index</h1>
<p>コーディネート一覧Find me in app/views/public/coordinates/index.html.erb</p>
<div>
  <% @coordinates.each do |coordinate| %>
    <%= link_to coordinate_path(coordinate.id) do %>
      <%= image_tag coordinate.get_coordinate_image(150,150) %>
    <% end %>
    <%= coordinate.tag.tag_name %>
    <%= coordinate.total_price %>
  <% end %>
</div>